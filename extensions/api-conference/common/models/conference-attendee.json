{
  "name": "ConferenceAttendee",
  "plural": "ConferenceAttendees",
  "description": "Conference: Manage Attendees in a Domain",
  "base": "SystemUser",
  "idInjection": true,
  "forceId": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Computed": {
      "properties": {
        "isSponsor": "isSponsor"
      }
    }
  },
  "properties": {
    "id": {
      "type": "String",
      "id": true,
      "required": true,
      "defaultFn": "uuidv4"
    },
    "email": {
      "type": "String",
      "required": true
    },
    "name": {
      "type": "String",
      "required": true
    },
    "firstName": {
      "type": "String",
      "required": false
    },
    "lastName": {
      "type": "String",
      "required": false
    },
    "password": {
      "type": "String",
      "required": true,
      "defaultFn": "uuidv4"
    },
    "company": {
      "type": "String"
    },
    "phone": {
      "type": "String"
    },
    "tags": {
      "type": "String"
    }
  },
  "hidden": [
    "fullName",
    "username",
    "firstName",
    "lastName"
  ],
  "relations": {
    "tickets": {
      "type": "hasMany",
      "model": "ConferenceTicket",
      "foreignKey": "conferenceAttendeeId"
    },
    "notes": {
      "type": "hasMany",
      "model": "ConferenceAttendeeNote",
      "foreignKey": "conferenceAttendeeId"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "SystemAccessToken",
      "polymorphic": {
        "foreignKey": "userId",
        "discriminator": "principalType"
      },
      "options": {
        "disableInclude": false
      }
    }
  },
  "validations": [],
  "acls": [],
  "methods": {}
}
